{
  "name": "coc-webpack",
  "version": "1.4.0",
  "description": "autocomplete support for webpack config",
  "keywords": [
    "coc.nvim",
    "webpack"
  ],
  "engines": {
    "coc": "^0.0.73"
  },
  "main": "./out/index.js",
  "repository": "https://github.com/iamcco/coc-webpack",
  "author": "iamcco <ooiss@qq.com>",
  "license": "MIT",
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "title": "coc-webpack config",
      "properties": {
        "coc-webpack.enable": {
          "type": "boolean",
          "default": true,
          "description": "enable coc-webpack"
        },
        "coc-webpack.disableWhenUseTypeCheck": {
          "type": "boolean",
          "default": true,
          "description": "disable coc-webpack when use @type {import('webpack').Configuration} comment"
        },
        "coc-webpack.trace.server": {
          "type": "string",
          "default": "off",
          "enum": [
            "off",
            "messages",
            "verbose"
          ],
          "description": "Trace level of coc-webpack"
        }
      }
    }
  },
  "scripts": {
    "build": "webpack"
  },
  "devDependencies": {
    "typescript": "^3.5.3",
    "@types/node": "^12.7.2",
    "coc.nvim": "^0.0.73",
    "colors": "^1.3.3",
    "ts-loader": "^6.0.4",
    "vscode-languageserver-types": "^3.14.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7"
  }
}
